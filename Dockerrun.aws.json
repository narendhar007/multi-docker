{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
	           "name": "client",
	           "image": "narendhart007/multi-client",
	           "hostname": "client",
	           "essential": false	   
		},
		{
                    "name": "server",
		    "image": "narendhar007/multi-server",
		    "hostname": "api",
		    "essential": false
	        },
		{
	            "name": "worker",
		    "image": "narendhar007/multi-worker",
		    "hostname": "worker",
		    "essential": false
		},
		{
		    "name": "nginx",
		    "image": "narendhar007/multi-nginx",
		    "hostname": "nginx",
		    "essential": true,
		    "portMappings": [
			    {
				    "hostPort": 80,
				    "containerPort": 80
		            }
		    ],
		    "links": ["client", "server"]
		}
	]
}
